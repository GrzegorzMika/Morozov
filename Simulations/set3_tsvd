docker build -t increasing_tsvd -f Dockerfile_increasing_tsvd .
docker build -t step_tsvd -f Dockerfile_step_tsvd .
docker build -t triangular_tsvd -f Dockerfile_triangular_tsvd .
docker build -t bimodal_tsvd -f Dockerfile_bimodal_tsvd .
docker build -t beta_tsvd -f Dockerfile_beta_tsvd .
docker build -t nm_tsvd -f Dockerfile_nm_tsvd .
